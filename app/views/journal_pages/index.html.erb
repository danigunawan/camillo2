<p id="notice"><%= notice %></p>

<h1>Journal Pages</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @journal_pages.each do |journal_page| %>
      <tr>
        <td><%= link_to 'Show', journal_page %></td>
        <td><%= link_to 'Edit', edit_journal_page_path(journal_page) %></td>
        <td><%= link_to 'Destroy', journal_page, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Journal Page', new_journal_page_path %>
